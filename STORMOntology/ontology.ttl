###  https://github.com/stormOntology/stormOntology.github.io/blob/master/storm#MM_01
:MM_01 rdf:type owl:NamedIndividual ;
       :belongs_to_work :Macbeth_play ;
       :enhances_isotopy :Insect ;
       :is_located_at :Presentation_of_enemies ;
       :refers_to_animal :Insect ;
       :serves_dialogue_purpose :Personal_traits ;
       :umbrella_metaphor :BAD_THINGS_ARE_NATURE ;
       <http://purl.org/spar/c4o/hasContent> "The multiplying villainies of nature do swarm upon him"@en ;
       :connotations "negative" ;
       :domains_connected_through "multiplicity" ;
       :has_interpretation "The subject has many flaws" ;
       :has_scholarly_interpretation "Nature here is seen as spawning unnumbered evils, and notable among them is the first of the evils defined in the play, rebellion, though this evil is later seen to be contrary to the better sort of Nature."@en ;
       :has_source_domain "insects" ;
       :has_starting_point "concrete" ;
       :has_target_domain "flaws" ;
       :has_utterance_type "dialogue" ;
       :in_sentence """The merciless Macdonald
Worthy to be a rebel, for to that
The multiplying villainies of nature
Do swarm upon him - from the Western Isles
Of kerns and galloglasses is supplied,
And Fortune on his damned quarrel smiling, Showed like a rebel's whore.""" ;
       :is_conceptual_metaphor "FLAWS ARE INSECTS" ;
       :is_found "at act 1, scene 2, line 12" .

